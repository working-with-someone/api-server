{
  "name": "wws-api-server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "https://github.com/seungho-hub/wws-api-server.git",
  "author": "seungho-hub <63999413+kangseungho-hub@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "watch": "dotenvx run --env-file=.env.development -- nodemon",
    "compile": "tsc",
    "start": "node dist/server.js",
    "db:migrate:prod": "dotenvx run --env-file=.env -- prisma migrate deploy",
    "db:migrate:dev": "dotenvx run --env-file=.env.development -- prisma migrate dev",
    "db:migrate:test": "dotenvx run --env-file=.env.test -- prisma migrate dev",
    "db:push:dev": "dotenvx run --env-file=.env.development -- prisma db push",
    "db:push:test": "dotenvx run --env-file=.env.test -- prisma db push",
    "db:seed:dev" : "dotenvx run --env-file=.env.development -- prisma db seed",
    "test": "dotenvx run --env-file=.env.test -- jest --runInBand",
    "test:unit": "dotenvx run --env-file=.env.test -- jest tests/unit",
    "test:integration": "dotenvx run --env-file=.env.test -- jest tests/integration --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/lib-storage": "^3.645.0",
    "@dotenvx/dotenvx": "^1.24.5",
    "@prisma/client": "6",
    "@types/cookie": "^0.6.0",
    "bcrypt": "^5.1.1",
    "connect-redis": "^7.1.1",
    "content-type": "^1.0.5",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-correlation-id": "^3.0.1",
    "express-session": "^1.18.0",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jest-mock-extended": "^3.0.7",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "lru-cache": "^11.1.0",
    "multer": "^1.4.5-lts.1",
    "prisma-error-enum": "^0.1.3",
    "redis": "^4.7.0",
    "supertest": "^7.0.0",
    "uuid": "^10.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/content-type": "^1.1.8",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.15",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-correlation-id": "^1.2.6",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lru-cache": "^7.10.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.4.1",
    "@types/redis": "^4.0.11",
    "@types/serve-favicon": "^2.5.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "^29.7.0",
    "node-mocks-http": "^1.16.1",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0",
    "prisma": "6",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
